{% macro create_aspect(name, rating) %}
<div class="spa-aspect">
    <div class="detail-title">{{ name }}</div>
    {% for i in range(0, rating) -%}
        <i class="fas fa-star spa-star"></i>
    {%- endfor %}
    {% for i in range(0, 5 - rating) -%}
        <i class="far fa-star"></i>
    {%- endfor %}
</div>
{% endmacro %}